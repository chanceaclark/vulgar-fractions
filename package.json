{
  "name": "vulgar-fractions",
  "description": "Turn integers into vulgar unicode fractions and vice-versa.",
  "keywords": [
    "vulgars",
    "fractions"
  ],
  "author": {
    "name": "Chancellor Clark",
    "email": "chancellorclark@gmail.com"
  },
  "homepage": "https://github.com/chanceaclark/vulgar-fractions#readme",
  "bugs": {
    "url" : "https://github.com/chanceaclark/vulgar-fractions/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chanceaclark/vulgar-fractions.git"
  },
  "version": "1.4.1",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:es && yarn build:dt",
    "build:cjs": "yarn swc src -d dist/cjs -C module.type=commonjs",
    "build:es": "yarn swc src -d dist/es -C module.type=es6",
    "build:dt": "yarn tsc --emitDeclarationOnly",
    "lint": "eslint ./src",
    "test": "jest",
    "prepublishOnly": "rm -rf dist && yarn lint && yarn build"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.194",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^28.1.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
